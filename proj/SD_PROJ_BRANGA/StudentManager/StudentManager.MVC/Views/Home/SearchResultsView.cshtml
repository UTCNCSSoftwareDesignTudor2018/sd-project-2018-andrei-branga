@model Restaurant.MVC.Models.SearchViewModel

<div>
    <h4>Query : CheckIn: @Model.startDate  |  CheckOut: @Model.endDate</h4>
    <hr />
    <dl class="dl-horizontal">
    </dl>
</div>

@Html.Action("RoomOffersResults",new
{
    start=Model.startDate,
    end=Model.endDate,
    roomType=Model.RoomType
})

<script>
    function CustomButtonClick2(s, e, url) {
        var key = s.GetRowKey(e.visibleIndex);


        if (e.buttonID === "btnGetKey") {
            var destUrl = url + "?offerId=" + key+"&start=" +'@Model.startDate.Ticks' +"&end="+'@Model.endDate.Ticks';
            window.location.href = destUrl;
        }
    }
</script>